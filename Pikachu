
package Pokemon;

public class Pikachu {
    int Health =1000;
    int Attack1 =100;
    int Attack2 =120;
    int Attack3 =140;
    int buff= 15;
    int exp=0;
    String Skill1 = "Tail Whip";
    String Skill2 = "Thunder Shock";
    String Skill3 = "Thunderbolt";
    String Skill4 = "Static";
    String Ket1 ="Basic Tackle Attack";
    String Ket2 ="Intermediate Attack using Thunder element";
    String Ket3 ="Strong Attack using Thunder element";
    String Ket4 ="Static Increase Hp by 15%.";
    
    
    public int get_health(){
        return Health;
    }
    public int get_att1(){
        return Attack1;
    }
    public int get_att2(){
        return Attack2;
    }
    public int get_att3(){
        return Attack3;
    }
    public String get_skill1(){
        return Skill1;
    }
    public String get_skill2(){
        return Skill2;
    }
    public String get_skill3(){
        return Skill3;
    }
    public String get_skill4(){
        return Skill4;
    }
    public int get_exp(){
        return exp;
    }
    public int get_buff(){
        return buff;
    }
    public String get_ket1(){
        return Ket1;
    }
    public String get_ket2(){
        return Ket2;
    }
    public String get_ket3(){
        return Ket3;
    }
    public String get_ket4(){
        return Ket4;
    }
    
}
